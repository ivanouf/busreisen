        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_ZhL9kLbXEeGxNLpsZ0hsPA" name="readGewuenschterKunde" specification="_Zf8AYLbXEeGxNLpsZ0hsPA">
