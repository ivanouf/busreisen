          <ownedParameter xmi:id="_ZhHFFLbXEeGxNLpsZ0hsPA" type="_ZPmeULbXEeGxNLpsZ0hsPA" direction="return"/>
          <language>java</language>
          <body>Buchung b = new Buchung();&#xD;
