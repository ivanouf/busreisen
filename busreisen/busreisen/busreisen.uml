        <ownedAttribute xmi:id="_Zgn84bbXEeGxNLpsZ0hsPA" name="reise" visibility="private" type="_ZQJQ4LbXEeGxNLpsZ0hsPA" association="_Zhp3obbXEeGxNLpsZ0hsPA">
