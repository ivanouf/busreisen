        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_ZhL9lLbXEeGxNLpsZ0hsPA" name="readGewuenschtesReiseziel" specification="_ZRpFsLbXEeGxNLpsZ0hsPA">
